<p id="notice"><%= notice %></p>

<h1>Ha Models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ha_models.each do |ha_model| %>
      <tr>
        <td><%= ha_model.name %></td>
        <td><%= ha_model.value %></td>
        <td><%= link_to 'Show', ha_model %></td>
        <td><%= link_to 'Edit', edit_ha_model_path(ha_model) %></td>
        <td><%= link_to 'Destroy', ha_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ha Model', new_ha_model_path %>
